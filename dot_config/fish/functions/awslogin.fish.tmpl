function awslogin
  op item get {{ .work.companyname }} --fields username > /dev/null ; or set -Ux OP_SESSION_$OP_ACCOUNT (op signin --acount $OP_ACCOUNT --raw)
  if test $status -gt 0
    printf "Unable to login.  Did you enter the correct password?"
  else
    op item get {{ .work.companyname }} --fields password | awssaml --password-stdin eks login
  end
end
